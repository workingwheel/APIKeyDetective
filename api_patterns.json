        "description": "Twitch API Key",
        "url": "https://dev.twitch.tv/docs/authentication"
    },
    "Yelp": {
        "pattern": "^[A-Za-z0-9]{32}$",
        "description": "Yelp API Key",
        "url": "https://www.yelp.com/developers/documentation/v3/authentication"
    },
    "Anthropic": {
        "pattern": "^sk-ant-api03-[A-Za-z0-9_-]{86,}$",
        "description": "Anthropic API Key",
        "url": "https://docs.anthropic.com/claude/docs/api-keys"
    }
