{
    "GitHub": {
        "pattern": "^gh[ps]_[A-Za-z0-9_]{36}$",
        "description": "GitHub Personal Access Token or GitHub SSH Key",
        "url": "https://docs.github.com/en/authentication/keeping-your-account-and-data-secure/creating-a-personal-access-token"
    },
    "Stripe": {
        "pattern": "^(sk|pk)_(test|live)_[A-Za-z0-9]+$",
        "description": "Stripe API Key (Secret or Publishable)",
        "url": "https://stripe.com/docs/api/authentication"
    },
    "AWS": {
        "pattern": "^AKIA[0-9A-Z]{16}$",
        "description": "AWS Access Key ID",
        "url": "https://docs.aws.amazon.com/general/latest/gr/aws-sec-cred-types.html"
    },
    "Google API": {
        "pattern": "^AIza[0-9A-Za-z\\-_]{35}$",
        "description": "Google API Key",
        "url": "https://cloud.google.com/docs/authentication/api-keys"
    },
    "OpenAI": {
        "pattern": "^sk-[A-Za-z0-9]{48}$",
        "description": "OpenAI API Key",
        "url": "https://platform.openai.com/docs/api-reference/authentication"
    },
    "Slack": {
        "pattern": "^xox[baprs]-([0-9]{12}-){3}[0-9a-zA-Z]{32}$",
        "description": "Slack API Token/Webhook",
        "url": "https://api.slack.com/authentication"
    },
    "Twilio": {
        "pattern": "^SK[0-9a-fA-F]{32}$",
        "description": "Twilio API Key",
        "url": "https://www.twilio.com/docs/usage/api"
    },
    "SendGrid": {
        "pattern": "^SG\\.[A-Za-z0-9_-]{22}\\.[A-Za-z0-9_-]{43}$",
        "description": "SendGrid API Key",
        "url": "https://sendgrid.com/docs/ui/account-and-settings/api-keys/"
    },
    "Dropbox": {
        "pattern": "^[A-Za-z0-9_-]{15}$",
        "description": "Dropbox API Key",
        "url": "https://www.dropbox.com/developers/reference/oauth-guide"
    },
    "Heroku": {
        "pattern": "^[0-9a-f]{32}$",
        "description": "Heroku API Key",
        "url": "https://devcenter.heroku.com/articles/platform-api-quickstart"
    },
    "DigitalOcean": {
        "pattern": "^[a-f0-9]{64}$",
        "description": "DigitalOcean API Token",
        "url": "https://www.digitalocean.com/docs/api/create-personal-access-token/"
    },
    "Mailgun": {
        "pattern": "^key-[0-9a-zA-Z]{32}$",
        "description": "Mailgun API Key",
        "url": "https://documentation.mailgun.com/en/latest/user_manual.html#api-key"
    },
    "Facebook": {
        "pattern": "^[0-9a-f]{32}$",
        "description": "Facebook Access Token",
        "url": "https://developers.facebook.com/docs/facebook-login/access-tokens/"
    },
    "Twitter": {
        "pattern": "^[A-Za-z0-9]{35,44}$",
        "description": "Twitter API Key",
        "url": "https://developer.twitter.com/en/docs/authentication/oauth-1-0a"
    },
    "LinkedIn": {
        "pattern": "^[A-Za-z0-9]{16}$",
        "description": "LinkedIn API Key",
        "url": "https://docs.microsoft.com/en-us/linkedin/shared/authentication/authentication"
    },
    "Azure": {
        "pattern": "^[0-9a-f]{32}$",
        "description": "Azure API Key",
        "url": "https://docs.microsoft.com/en-us/azure/azure-resource-manager/management/authenticate-interactive"
    },
    "PayPal": {
        "pattern": "^[A-Za-z0-9]{20}$",
