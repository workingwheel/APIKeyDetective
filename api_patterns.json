    },
    "Pinterest": {
        "pattern": "^[A-Za-z0-9]{32}$",
        "description": "Pinterest API Key",
        "url": "https://developers.pinterest.com/docs/getting-started/introduction/"
    },
    "Etsy": {
        "pattern": "^[A-Za-z0-9]{32}$",
        "description": "Etsy API Key",
